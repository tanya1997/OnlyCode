{
  "name": "webserver",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "content-disposition": "^0.5.4",
    "cookie": "^0.6.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-http-proxy": "^2.0.0",
    "grommet": "^2.38.0",
    "grommet-icons": "^4.12.1",
    "mime-types": "^2.1.35",
    "node-fetch": "^2.7.0",
    "python-bridge": "^1.1.0",
    "query-string": "^9.0.0",
    "raw-body": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:backend start:frontend",
    "start:backend": "nodemon api/index.mjs",
    "start:frontend": "cross-env PORT=3001 BROWSER=none nodemon --exec \"react-scripts build\" --watch src",
    "lint": "eslint --quiet",
    "prettier": "prettier '**/*.{svg,yml,js,ts,jsx,tsx,html,md,json,mjs}' --write",
    "ml:server:start": "node ./ml-start.mjs",
    "ml:server:stop": "node ./ml-stop.mjs",
    "ml:server:status": "node ./ml-status.mjs"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,yml}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
