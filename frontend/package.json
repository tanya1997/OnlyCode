{
  "name": "frontend",
  "version": "0.1.0",
  "dependencies": {
    "dotenv": "^16.4.5",
    "grommet": "^2.38.0",
    "grommet-icons": "^4.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "^5.0.1",
    "styled-components": "^6.1.11",
    "query-string": "^9.0.0",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "react-redux": "^9.1.2",
    "js-cookie": "^3.0.5",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/redux-saga": "^0.10.5"
  },
  "scripts": {
    "start": "dotenvx run --env-file=.env.default --env-file=.env -- react-scripts start",
    "build": "dotenvx run --env-file=.env.default --env-file=.env -- react-scripts build"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,yml}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
